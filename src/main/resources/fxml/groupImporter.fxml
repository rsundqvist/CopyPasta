<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity"
            minWidth="-Infinity" prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="zip.GroupImporterController">
    <center>
        <TabPane prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
            <tabs>
                <Tab text="Summary">
                    <content>
                        <GridPane hgap="5.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                            <columnConstraints>
                                <ColumnConstraints hgrow="NEVER" minWidth="10.0" prefWidth="125.0"/>
                                <ColumnConstraints hgrow="NEVER" minWidth="10.0" prefWidth="125.0"/>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="125.0"/>
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="20.0"
                                                vgrow="SOMETIMES"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                            </rowConstraints>
                            <children>
                                <ListView fx:id="groupListView" maxHeight="1.7976931348623157E308" maxWidth="-Infinity"
                                          minWidth="0.0" prefHeight="50.0" prefWidth="125.0" GridPane.rowIndex="1"
                                          GridPane.rowSpan="2147483647" GridPane.vgrow="ALWAYS"/>
                                <Label text="Groups"/>
                                <Label text="Files" GridPane.columnIndex="1"/>
                                <ListView fx:id="filesListView" maxHeight="1.7976931348623157E308" maxWidth="-Infinity"
                                          minWidth="0.0" prefHeight="50.0" prefWidth="125.0" GridPane.columnIndex="1"
                                          GridPane.rowIndex="1" GridPane.rowSpan="2147483647"/>
                                <BorderPane fx:id="previewContainer" prefHeight="200.0" prefWidth="200.0"
                                            GridPane.columnIndex="2" GridPane.columnSpan="2147483647"
                                            GridPane.hgrow="ALWAYS" GridPane.rowIndex="1" GridPane.rowSpan="2147483647"
                                            GridPane.vgrow="ALWAYS"/>
                                <HBox GridPane.columnIndex="2" GridPane.hgrow="ALWAYS">
                                    <children>
                                        <Label maxHeight="1.7976931348623157E308" text="File preview:"
                                               GridPane.columnIndex="2" HBox.hgrow="NEVER">
                                            <HBox.margin>
                                                <Insets right="5.0"/>
                                            </HBox.margin>
                                        </Label>
                                        <Label fx:id="filePreviewLabel" maxHeight="1.7976931348623157E308"
                                               maxWidth="1.7976931348623157E308" text="NULL" HBox.hgrow="ALWAYS">
                                            <font>
                                                <Font name="System Bold" size="12.0"/>
                                            </font>
                                        </Label>
                                    </children>
                                </HBox>
                            </children>
                        </GridPane>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
    </center>
    <right>
        <VBox>
            <children>
                <TreeView fx:id="treeView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                          minHeight="0.0" minWidth="0.0" prefHeight="150.0" prefWidth="200.0"
                          BorderPane.alignment="CENTER" VBox.vgrow="ALWAYS"/>
                <GridPane hgap="5.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                          minWidth="-Infinity" prefHeight="200.0" VBox.vgrow="NEVER">
                    <columnConstraints>
                        <ColumnConstraints hgrow="NEVER" minWidth="10.0" prefWidth="125.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="20.0" vgrow="NEVER"/>
                        <RowConstraints maxHeight="1.7976931348623157E308" minHeight="10.0" prefHeight="30.0"
                                        vgrow="ALWAYS"/>
                    </rowConstraints>
                    <children>
                        <Label text="File patterns"/>
                        <TextArea maxHeight="1.7976931348623157E308" maxWidth="-Infinity" minHeight="0.0" minWidth="0.0"
                                  prefWidth="125.0" text="*.java&#10;*.txt" GridPane.rowIndex="1"
                                  GridPane.rowSpan="2147483647"/>
                        <VBox prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
                            <children>
                                <CheckBox mnemonicParsing="false" onAction="#onToggleOpenArchives" selected="true"
                                          text="Open archives"/>
                                <Pane prefHeight="200.0" prefWidth="200.0"/>
                                <Button maxHeight="-Infinity" maxWidth="1.7976931348623157E308" minHeight="-Infinity"
                                        mnemonicParsing="false" onAction="#onImport" prefHeight="40.0" text="Import">
                                    <font>
                                        <Font name="System Bold" size="18.0"/>
                                    </font>
                                </Button>
                            </children>
                        </VBox>
                    </children>
                </GridPane>
            </children>
        </VBox>
    </right>
    <opaqueInsets>
        <Insets/>
    </opaqueInsets>
    <top>
        <HBox maxWidth="1.7976931348623157E308" prefWidth="200.0" BorderPane.alignment="CENTER">
            <children>
                <Button mnemonicParsing="false" onAction="#onChangeRootDirectory" text="Root directory"/>
                <TextField fx:id="rootDirectoryField" editable="false" maxWidth="1.7976931348623157E308"
                           promptText="No root directory selected." HBox.hgrow="ALWAYS">
                    <HBox.margin>
                        <Insets left="5.0" right="5.0"/>
                    </HBox.margin>
                </TextField>
            </children>
            <padding>
                <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
            </padding>
        </HBox>
    </top>
    <padding>
        <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
    </padding>
</BorderPane>
