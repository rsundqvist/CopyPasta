<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox xmlns:fx="http://javafx.com/fxml/1" maxHeight="1.7976931348623157E308" maxWidth="-Infinity" minWidth="300.0"
      xmlns="http://javafx.com/javafx/8">
    <children>
        <GridPane maxWidth="1.7976931348623157E308">
            <children>
                <Label minWidth="-Infinity" text="#Total" GridPane.rowIndex="2">
                    <font>
                        <Font name="System Bold" size="12.0"/>
                    </font>
                </Label>
                <Label minWidth="-Infinity" text="#Complete" GridPane.rowIndex="3">
                    <font>
                        <Font name="System Bold" size="12.0"/>
                    </font>
                </Label>
                <Label minWidth="-Infinity" text="#Remaining" GridPane.rowIndex="4">
                    <font>
                        <Font name="System Bold" size="12.0"/>
                    </font>
                </Label>
                <Label fx:id="numFeedback" alignment="CENTER_RIGHT" maxWidth="1.7976931348623157E308"
                       minWidth="-Infinity" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="2">
                    <GridPane.margin>
                        <Insets right="2.0"/>
                    </GridPane.margin>
                </Label>
                <Label fx:id="numDone" alignment="CENTER_RIGHT" maxWidth="1.7976931348623157E308" minWidth="-Infinity"
                       text="Label" GridPane.columnIndex="1" GridPane.rowIndex="3">
                    <GridPane.margin>
                        <Insets right="2.0"/>
                    </GridPane.margin>
                </Label>
                <Label fx:id="numNotDone" alignment="CENTER_RIGHT" maxWidth="1.7976931348623157E308"
                       minWidth="-Infinity" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="4">
                    <GridPane.margin>
                        <Insets right="2.0"/>
                    </GridPane.margin>
                </Label>
                <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="Feedback Status"
                       GridPane.columnSpan="2147483647">
                    <font>
                        <Font name="System Bold" size="14.0"/>
                    </font>
                </Label>
                <HBox GridPane.columnSpan="2147483647" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1">
                    <children>
                        <ProgressBar fx:id="progressBar" maxWidth="1.7976931348623157E308" minHeight="-Infinity"
                                     prefHeight="20.0" prefWidth="138.0" progress="0.0" GridPane.columnSpan="2147483647"
                                     GridPane.hgrow="ALWAYS" GridPane.rowIndex="1" HBox.hgrow="ALWAYS">
                            <HBox.margin>
                                <Insets right="5.0"/>
                            </HBox.margin>
                        </ProgressBar>
                        <Label fx:id="progressLabel" alignment="CENTER" contentDisplay="CENTER"
                               maxHeight="1.7976931348623157E308" maxWidth="-Infinity" minHeight="-Infinity"
                               minWidth="-Infinity" prefWidth="35.0" text="100 %" textAlignment="CENTER"
                               GridPane.columnIndex="1" GridPane.columnSpan="2147483647" GridPane.rowIndex="1"
                               HBox.hgrow="NEVER">
                            <font>
                                <Font name="System Bold Italic" size="12.0"/>
                            </font>
                        </Label>
                    </children>
                </HBox>
            </children>
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
            </rowConstraints>
        </GridPane>
        <HBox VBox.vgrow="ALWAYS">
            <children>
                <VBox prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
                    <children>
                        <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="Complete" VBox.vgrow="NEVER">
                            <font>
                                <Font name="System Bold" size="12.0"/>
                            </font>
                        </Label>
                        <ListView fx:id="doneListView" maxHeight="1.7976931348623157E308"
                                  maxWidth="1.7976931348623157E308" minHeight="0.0" minWidth="0.0" prefHeight="200.0"
                                  prefWidth="200.0" VBox.vgrow="ALWAYS"/>
                        <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#exportAllDone"
                                text="Export All" VBox.vgrow="NEVER"/>
                        <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#clearDone"
                                text="Clear" VBox.vgrow="NEVER"/>
                    </children>
                </VBox>
                <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="200.0"
                      prefWidth="100.0" HBox.hgrow="ALWAYS">
                    <children>
                        <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="Remaining" VBox.vgrow="NEVER">
                            <font>
                                <Font name="System Bold" size="12.0"/>
                            </font>
                        </Label>
                        <ListView fx:id="notDoneListView" maxHeight="1.7976931348623157E308"
                                  maxWidth="1.7976931348623157E308" minHeight="0.0" minWidth="0.0" prefHeight="200.0"
                                  prefWidth="200.0" VBox.vgrow="ALWAYS"/>
                        <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#exportAllNotDone"
                                text="Export All" VBox.vgrow="NEVER"/>
                        <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#clearAllNotDone"
                                text="Clear" VBox.vgrow="NEVER"/>
                    </children>
                </VBox>
            </children>
        </HBox>
    </children>
</VBox>
